{
  "name": "it-data-flow-simulator",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "hmr": "ng serve --configuration hmr -o",
    "server": "tsc-watch --onSuccess \"node ./server/server.js --port 3000\"  --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc -p ./server/",
    "svg": "gulp svg",
    "start-mock": "npm run svg && ng serve --hmr --open --port 4201 --configuration hmr -o",
    "start-local": "npm run svg && ng serve --open --port 4201",
    "build": "ng build --prod --buildOptimizer",
    "build:server": "webpack-dev-server --open --port 3000 --entry /webpack-dev-server --output-path dist/it-data-flow-simulator --compress --content-base dist/it-data-flow-simulator --history-api-fallback",
    "test": "ng test",
    "type-replace": "gulp type-replace",
    "copy": "copyfiles -V -E -f ./dist/* ../src/main/resources/static/",
    "deploy": "npm run build && npm run copy",
    "less": "gulp less && gulp less:inject",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "report": "webpack-bundle-analyzer dist/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.8",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/forms": "^11.0.8",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "@datorama/akita": "^6.1.3",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@ngneat/until-destroy": "^8.0.4",
    "copyfiles": "^2.3.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "~11.2.7",
    "@angular/compiler-cli": "~11.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "body-parser": "^1.19.0",
    "codelyzer": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tsc-watch": "^3.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.4.1"
  }
}
